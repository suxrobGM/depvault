{
  "name": "@depvault/backend",
  "module": "src/index.js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "bun run test",
    "dev": "bun run --watch src/app.ts",
    "start": "bun run src/app.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev --create-only",
    "db:migrate:apply": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "build:linux": "bun build --compile --minify-whitespace --minify-syntax --target bun-linux-x64 --outfile dist/server src/app.ts",
    "build:win": "bun build --compile --minify-whitespace --minify-syntax --target bun-windows-x64 --outfile dist/server.exe src/app.ts",
    "build:types": "tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/static": "^1.4.7",
    "@elysiajs/swagger": "^1.3.1",
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "@prisma/client-runtime-utils": "^7.4.1",
    "@sinclair/typebox": "^0.34.48",
    "dotenv": "^17.3.1",
    "elysia": "^1.4.25",
    "jose": "^6.1.3",
    "pg": "^8.18.0",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@types/pg": "^8.16.0",
    "bun-types": "^1.3.9",
    "prisma": "^7.4.1",
    "typescript": "^5.9.3"
  }
}
